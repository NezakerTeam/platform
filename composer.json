{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "fideloper/proxy": "~3.3",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5.0",
        "kris/laravel-form-builder": "^1.9",
        "laravelcollective/annotations": "dev-master",
        "google/apiclient": "^2.0",
        "backpack/base": "^0.8",
        "backpack/crud": "^3.3",
        "aws/aws-sdk-php": "~3.0",
        "backpack/permissionmanager": "^2.1",
        "artesaos/seotools": "^0.10.0",
        "backpack/backupmanager": "^1.1",
        "dingo/api": "1.0.x@dev",
        "backpack/pagemanager": "^1.1",
        "cviebrock/eloquent-sluggable": "^4.4"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "backpack/generators": "^1.1",
        "xethron/migrations-generator": "^2.0",
        "krlove/eloquent-model-generator": "^1.2",
        "barryvdh/laravel-debugbar": "^2.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": false,
        "optimize-autoloader": true
    },
    "repositories": {
        "repo-name": {
            "type": "vcs",
            "url": "https://github.com/bulldesk/annotations.git"
        }
    }
}
